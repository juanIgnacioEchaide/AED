#include <iostream>


double Celsius(double);

int main(){
	
	
	assert(0== Celsius(32));
	assert(-17.77==Celsius(0));
	assert(-35.55==Celsius(-32));
	assert(37.77==Celsius(100));
	assert(-73.33== Celsius(-100));


double Celsius(double f){ 
	return (5.0/9.0)*(f-32.0); 
}	

}
